void insertionSort(int *vetor, int n){
    int i, j, atual;

    for(i = 1; i < n; i++){
        atual = vetor[i];
        for(j = i; (j > 0) && (atual < vetor[j - 1]); j--){
            vetor[j] = vetor[j - 1];
        }
        vetor[j] = atual;
    }

}

int main(){
    int vetor[5] = {5, 3, 2 , 4, 1};

    insertionSort(vetor, 5);

    for(int i = 0; i < 5; i++){
        printf("%d ", vetor[i]);
    }

  return 0;
}
