#include <stdio.h>
#include <stdlib.h>


int main()
{
    int num1, num2;
    printf("Digite dois numeros inteiros e imprima na ordem inversa: ");
    scanf("%d", &num1);
    scanf("%d", &num2);
    printf("Numeros escritos em ordem inversa da leitura: %d - %d", num2, num1);

    return 0;
}
