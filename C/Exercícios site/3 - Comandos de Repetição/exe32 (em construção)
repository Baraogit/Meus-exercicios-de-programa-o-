#include <stdio.h>


int main(){
    int i, numeroDeLancamentos, d1 = 0, d2 = 0;

    printf("Informe o numero de lancamentos dos dados: ");
    scanf("%d", &numeroDeLancamentos);

    srand(time(NULL));


    for(i = 0; i < numeroDeLancamentos; i++){
        d1 = rand()  % 6 ;
        d2 = rand()  % 6 ;
        printf("Dado 1 = %d\n", d1);
        printf("Dado 2 = %d\n", d2);

    }

    return 0;
}
