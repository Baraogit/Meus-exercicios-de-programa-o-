#include <stdio.h>
#include <math.h>

int main(){
    double doisPrimeirosDigitosDe_i, doisUltimosDigitosDe_i, quadradoDosDigitos;
    int i;

    for (i = 1000; i <= 9999; i++){
        doisPrimeirosDigitosDe_i = i / 100;
        doisUltimosDigitosDe_i = i % 100;
        quadradoDosDigitos = pow(doisPrimeirosDigitosDe_i + doisUltimosDigitosDe_i, 2);
        printf("%.0f\n", quadradoDosDigitos);
    }
    return 0;
}



